    </issueManagement>

    <ciManagement>
        <system>Travis</system>
        <url>https://travis-ci.org/jayway/maven-android-plugin</url>
    </ciManagement>

    <distributionManagement>
        <snapshotRepository>
            <id>ossrh</id>
            <name>Jayway OpenSource SNAPSHOTs on Sonatype.org</name>
